{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "react-app",
        "react-app/jest",
        "plugin:react/jsx-runtime"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": "off",
        "linebreak-style": "off",
        "max-len":"off",
        "react/prop-types":"off",
        "jsx-a11y/click-events-have-key-events":"off",
        "jsx-a11y/control-has-associated-label":"off",
        "jsx-a11y/no-static-element-interactions":"off"
    }
}
